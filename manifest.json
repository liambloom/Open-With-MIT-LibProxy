{
    "name": "Open with MIT Library Proxy",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "An extension that opens the current page using the MIT Library proxy",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "Open Proxy"
    },
    "background": {
        "service_worker": "background.js"
    }
}